{
  "name": "self-track",
  "version": "1.0.0",
  "description": "An Electron application with Svelte and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run electron-rebuild && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "check-packages": "npx npm-check -u -E",
    "refresh-packages": "rm -rf node_modules && rm -rf package-lock.json && npm i",
    "electron-rebuild": "electron-rebuild -f -w yourmodule"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.1",
    "@electron-toolkit/utils": "4.0.0",
    "csv-parser": "3.2.0",
    "electron-updater": "6.6.2",
    "koffi": "2.10.1",
    "luxon": "3.6.0",
    "node-desktop-idle-v2": "0.0.17",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "3.0.0",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@electron/rebuild": "3.7.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/luxon": "3.4.2",
    "@types/node": "22.13.14",
    "@types/uuid": "10.0.0",
    "electron": "35.1.2",
    "electron-builder": "26.0.12",
    "electron-vite": "3.1.0",
    "eslint": "9.23.0",
    "eslint-plugin-svelte": "3.4.0",
    "npm-check": "6.0.1",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "svelte": "5.25.3",
    "svelte-check": "4.1.5",
    "typescript": "5.8.2",
    "vite": "6.2.4"
  }
}
