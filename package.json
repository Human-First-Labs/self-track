{
  "name": "self-track",
  "version": "1.0.1",
  "description": "An open source self-tracking application for macOS, Windows, and Linux.",
  "main": "./out/main/index.js",
  "author": "Human First Labs (HFL)",
  "homepage": "https://human-first-labs.com/self-track",
  "repository": "https://github.com/MomoRazor/self-track",
  "scripts": {
    "test": "jest",
    "test:setup": "ts-jest config:init",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "electron-rebuild": "electron-rebuild -f",
    "build": "rm -rf dist && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:all": "npm run build && electron-builder --win && electron-builder --linux",
    "check-packages": "npx npm-check -u -E",
    "refresh-packages": "rm -rf node_modules && rm -rf package-lock.json && npm i"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.2",
    "@electron-toolkit/utils": "4.0.0",
    "csv-parser": "3.2.0",
    "electron-updater": "6.6.2",
    "exceljs": "4.4.0",
    "koffi": "2.13.0",
    "luxon": "3.7.1",
    "node-desktop-idle-v2": "file:node-desktop-idle-v2-1.1.6.tgz",
    "ts-jest": "29.4.1",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "3.1.0",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@sveltejs/vite-plugin-svelte": "6.1.1",
    "@types/jest": "30.0.0",
    "@types/luxon": "3.7.1",
    "@types/node": "24.2.1",
    "@types/uuid": "10.0.0",
    "electron": "37.2.6",
    "electron-builder": "26.0.12",
    "electron-vite": "4.0.0",
    "eslint": "9.33.0",
    "eslint-plugin-svelte": "3.11.0",
    "jest": "30.0.5",
    "npm-check": "6.0.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.38.0",
    "svelte-check": "4.3.1",
    "typescript": "5.9.2",
    "vite": "7.1.1"
  }
}
