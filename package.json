{
  "name": "self-track",
  "version": "1.0.0",
  "description": "An open source self-tracking application for macOS, Windows, and Linux.",
  "main": "./out/main/index.js",
  "author": "Human First Labs (HFL)",
  "homepage": "https://human-first-labs.com/self-track",
  "repository": "https://github.com/MomoRazor/self-track",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "electron-rebuild": "electron-rebuild -f",
    "build": "rm -rf dist && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:all": "npm run build && electron-builder --win && electron-builder --linux",
    "check-packages": "npx npm-check -u -E",
    "refresh-packages": "rm -rf node_modules && rm -rf package-lock.json && npm i"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.1",
    "@electron-toolkit/utils": "4.0.0",
    "csv-parser": "3.2.0",
    "electron-updater": "6.6.2",
    "exceljs": "^4.4.0",
    "koffi": "2.10.1",
    "luxon": "3.6.1",
    "node-desktop-idle-v2": "0.0.18",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "3.0.0",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/luxon": "3.6.2",
    "@types/node": "22.14.0",
    "@types/uuid": "10.0.0",
    "electron": "35.1.4",
    "electron-builder": "26.0.12",
    "electron-vite": "3.1.0",
    "eslint": "9.24.0",
    "eslint-plugin-svelte": "3.5.1",
    "npm-check": "6.0.1",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "svelte": "5.25.8",
    "svelte-check": "4.1.5",
    "typescript": "5.8.3",
    "vite": "6.2.5"
  }
}
